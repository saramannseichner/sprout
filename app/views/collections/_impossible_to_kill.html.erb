<div class="collection">
  <h3>IMPOSSIBLE TO KILL</h3>
  <% results = [] %>
  <% plants.each do |plant| %>
    <% if plant.care_level == 1 %>
        <% results << plant %>
    <% end %>
  <% end %>
  <%= render partial: 'components/plant', collection: results, as: :plant, locals: { card_height: "260px", img_height: 260, pos: "1" } %>
</div>
